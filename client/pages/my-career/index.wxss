#box{
  padding:50rpx 20rpx;
  background-color:#f5f5f5;
  overflow:hidden;
}
.career-panel{
  background-color:#fff;
  width:90%;
  margin:0 auto 20rpx;
  padding:20rpx;
  border-radius:5rpx;
}
.panel-title{
  display:block;
  font-size:40rpx;
  font-family: "MICROSOFTYAHEI";
  font-weight:bold;
  border-bottom:4rpx solid #f5f5f5;
  padding:20rpx;
}
.content{
  font-size:30rpx;
  font-weight: 500;
  padding:10rpx 0 30rpx 0;
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
} 
.content::after{
  display:block;

}
.info{
   font-size:30rpx;
  font-weight: 400;
  border-bottom:4rpx solid #f5f5f5;
  padding-bottom:20rpx;
}
.right{
  float:right;
}

/* 提示的三个颜色 */

.info-success{
  color:green;
}
.info-pro{
  color:blue;
}
.info-failed{
  color:Red;
}
.time{
    font-size:30rpx;
    text-align: right;
    display:block;
}